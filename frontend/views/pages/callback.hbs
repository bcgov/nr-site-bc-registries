{{#> template}}
  {{#*inline "content"}}
    <main role="main">
      <div class="container">
        <div class="row">
          <div class="col-sm">
              <div>Authentication successful! Redirecting in <span id="countdown">5</span></div>
          </div>
        </div>
      </div>
    </main>
    <script type="text/javascript" defer>
    window.onload = function() {
        getAccessCode().then(countdown());
    }
    </script>
  {{/inline}}
{{/template}}